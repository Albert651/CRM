{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {"runtime":"vercel-php@0.6.0"}
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "https://your-domain.vercel.app",
      "DB_CONNECTION": "mysql",
      "DB_HOST": "db",
      "DB_PORT": "@db_port",
      "DB_DATABASE": "@db_database",
      "DB_USERNAME": "@db_username",
      "DB_PASSWORD": "@db_password"
    }
  }
  
